{
  "name": "bc-minecraft-bedrock-diagnoser",
  "version": "1.21.135-0",
  "description": "A typescript package library that provides diagnostics for minecraft bedrock projects",
  "author": {
    "name": "Blockception Ltd",
    "url": "https://www.blockception.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blockception/minecraft-bedrock-language-server",
    "directory": "packages/bedrock-diagnoser"
  },
  "bugs": {
    "url": "https://github.com/Blockception/minecraft-bedrock-language-server/issues"
  },
  "homepage": "https://github.com/Blockception/minecraft-bedrock-language-server#readme",
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "./dist",
    "../../../LICENSE"
  ],
  "scripts": {
    "compile": "tsc -b --pretty ./tsconfig.json",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "test": "jest --config ./jest.config.ts",
    "lint": "eslint --config ../../eslint.config.mjs .",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "pretest": "npm run compile",
    "prepublishOnly": "npm test && npm run compile",
    "preversion": "",
    "version": "git add -A src",
    "watch": "tsc -w"
  },
  "dependencies": {
    "bc-minecraft-bedrock-shared": "*",
    "bc-minecraft-bedrock-command": "*",
    "bc-minecraft-bedrock-project": "*",
    "bc-minecraft-bedrock-types": "*",
    "bc-minecraft-bedrock-vanilla-data": "*",
    "bc-minecraft-molang": "*",
    "bc-minecraft-project": "*"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "^24.3.1",
    "eslint": "^10.0.2",
    "rimraf": "^6.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.56.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  }
}
